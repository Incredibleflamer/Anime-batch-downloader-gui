{
  "name": "animedownloader",
  "version": "3.2.0",
  "description": "Download anime in batches & its fast :3",
  "main": "gui.js",
  "scripts": {
    "start": "electron . --trace-warnings",
    "package": "electron-builder",
    "start:prod": "cross-env NODE_ENV=production electron .",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheYogMehta/Anime-batch-downloader-gui.git"
  },
  "build": {
    "appId": "com.animebatchdownloader.gui",
    "win": {
      "target": [
        "nsis"
      ],
      "artifactName": "animedownloader.Setup.${version}.exe",
      "icon": "assets/luffy.ico"
    },
    "publish": {
      "provider": "github",
      "owner": "TheYogMehta",
      "repo": "Anime-batch-downloader-gui",
      "private": false
    },
    "nsis": {
      "deleteAppDataOnUninstall": true
    },
    "icon": "assets/luffy.ico",
    "compression": "maximum",
    "asarUnpack": [
      "node_modules/ffmpeg-static/**/*",
      "assets/**/*"
    ]
  },
  "keywords": [
    "anime",
    "animedownloader",
    "animebatchdownloader"
  ],
  "author": {
    "name": "TheYogMehta",
    "email": "mehtayog5@gmail.com",
    "url": "https://github.com/TheYogMehta"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "better-sqlite3": "^11.8.1",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0",
    "crypto-js": "^4.2.0",
    "discord-rpc": "^4.0.1",
    "ejs": "^3.1.9",
    "electron-updater": "^6.3.9",
    "express": "^4.19.1",
    "ffmpeg-static": "^5.2.0",
    "fs-extra": "^10.0.0",
    "got": "^11.8.6",
    "image-pixels": "^2.2.2",
    "iso-639-1": "^3.1.5",
    "jszip": "^3.10.1",
    "node-cache": "^5.1.2",
    "simpl.db": "^2.13.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "electron": "^35.0.0",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9"
  }
}
